<% @favorites.each do |fav| %>
  <div class="panel">
    <div class="panel-heading">
      <%= fav.picture.title %>
    </div>
    <div class="panel-body">
      <%= image_tag(fav.picture.image.url) %>
    </div>
    <div class="panel-footer">
      <%= fav.picture.comment %>
    </div>
  </div>
<% end %>