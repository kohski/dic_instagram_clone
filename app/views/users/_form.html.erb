<%= form_with(model: @user,local:true) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>  
  </div>
  <div>
    <%= f.label :image %>
    <%= f.hidden_field :image_cache %>
    <%= f.file_field :image %>    
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :password_confirmation %>
    <%= f.password_field :pasword_confirmation %>  
  </div>

  <div>
    <%= f.submit %>
  </div>


<% end %>