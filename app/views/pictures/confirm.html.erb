<h1>Confirm Post picture</h1>
<%= form_with(model: @picture,local: true,url:pictures_path) do |f| %>
  <div>
    <%= @picture.title %>
  </div>
  <div>
    <%= image_tag(@picture.image.url ) if @picture.image && @picture.image.url %>
  </div>
  <div>
    <%= @picture.comment %>
  </div>
  <%= f.hidden_field :title %>
  <%= f.hidden_field :image_cache %>
  <%= f.hidden_field :comment %>
  <%= f.submit %>
<% end %>

